generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BlogPost {
  id        String  @id @default(cuid())
  title     String
  subtitle  String
  heroImage String
  content   Json
}

model User {
  id        String  @id @default(cuid())
  email     String
  password  String
}
